{
  "name": "dynamic-data-types",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "ts-interface-checker": "^0.1.9"
  },
  "scripts": {
    "start": "npm run build-interfaces && react-scripts start",
    "build": "npm run build-interfaces && react-scripts build",
